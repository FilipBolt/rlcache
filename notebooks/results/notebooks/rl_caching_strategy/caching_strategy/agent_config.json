{
  "name": "caching_dqn_network",
  "type": "dqn",
  "memory_spec": {
    "type": "prioritized_replay",
    "capacity": 1000
  },
  "policy_spec": {},
  "network_spec": [
    {
      "type": "dense",
      "units": 128,
      "activation": "relu",
      "scope": "hidden"
    },
    {
      "type": "dense",
      "units": 128,
      "activation": "relu",
      "scope": "hidden2"
    }
  ],
  "preprocessing_spec": [],
  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0003
  },
  "update_spec": {
    "do_updates": true,
    "update_interval": 4,
    "steps_before_update": 500,
    "batch_size": 512,
    "sync_interval": 500
  },
  "exploration_spec": {
    "epsilon_spec": {
      "decay_spec": {
        "type": "linear_decay",
        "from": 1.0,
        "to": 0.2
      }
    }
  }
}