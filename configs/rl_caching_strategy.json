{
  "experiment_name": "rl_caching_strategy_only",
  "cache_backend_settings": {
    "type": "cache_inmemory",
    "capacity": 500
  },
  "database_backend_settings": {
    "type": "inmemory",
    "capacity": 1000000000
  },
  "cache_manager_settings": {
    "experiment_name": "rl_caching_strategy_only",
    "caching_strategy_settings": {
      "type": "rl_driven",
      "num_fields": 10,
      "observer_storage_capacity": 100000,
      "agent_config": {
        "name": "RLCachingStrategy",
        "type": "dqn",
        "memory_spec": {
          "type": "replay",
          "capacity": 128
        },
        "network_spec": [
          {
            "type": "dense",
            "units": 8,
            "activation": "relu",
            "scope": "hidden"
          }
        ],
        "preprocessing_spec": [],
        "optimizer_spec": {
          "type": "adam",
          "learning_rate": 0.02
        },
        "execution_spec": {
          "seed": 15,
          "gpu_spec": {
            "gpus_enabled": true,
            "max_usable_gpus": 1,
            "allow_memory_growth": true
          },
          "disable_monitoring": true,
          "session_config": {
            "allow_soft_placement": true,
            "device_count": {
              "CPU": 1
            },
            "inter_op_parallelism_threads": 1,
            "intra_op_parallelism_threads": 1
          }
        },
        "exploration_spec": {
          "epsilon_spec": {
            "decay_spec": {
              "type": "linear_decay",
              "from": 1.0,
              "to": 0.05,
              "start_timestep": 0,
              "num_timesteps": 1500
            }
          }
        }
      }
    },
    "ttl_strategy_settings": {
      "type": "fixed",
      "ttl": 2
    },
    "eviction_strategy_settings": {
      "type": "lru"
    }
  }
}