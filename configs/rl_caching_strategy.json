{
  "experiment_name": "rl_caching_strategy_only_no_episodes",
  "cache_backend_settings": {
    "type": "cache_inmemory",
    "capacity": 50000
  },
  "database_backend_settings": {
    "type": "inmemory"
  },
  "cache_manager_settings": {
    "caching_strategy_settings": {
      "type": "rl_driven",
      "checkpoint_steps": 10000,
      "state_configs": {
        "value_field_length": 10
      },
      "agent_config": {
        "name": "RLCachingStrategy",
        "type": "dqn",
        "memory_spec": {
          "type": "prioritized_replay",
          "capacity": 1000
        },
        "observe_spec": {
          "buffer_size": 1000
        },
        "network_spec": [
          {
            "type": "dense",
            "units": 64,
            "activation": "relu",
            "scope": "hidden"
          }
        ],
        "preprocessing_spec": [],
        "optimizer_spec": {
          "type": "adam",
          "learning_rate": 0.003
        },
        "execution_spec": {
          "gpu_spec": {
            "gpus_enabled": false
          }
        },
        "update_spec": {
          "do_updates": true,
          "update_interval": 4,
          "steps_before_update": 50000,
          "batch_size": 512,
          "sync_interval": 500000
        },
        "exploration_spec": {
          "epsilon_spec": {
            "decay_spec": {
              "type": "linear_decay",
              "from": 1.0,
              "to": 0.05,
              "start_timestep": 0,
              "num_timesteps": 500000
            }
          }
        }
      }
    },
    "ttl_strategy_settings": {
      "type": "fixed",
      "ttl": 120
    },
    "eviction_strategy_settings": {
      "type": "lru"
    }
  }
}